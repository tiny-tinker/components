{
  "name": "asyncy-vue-components",
  "version": "0.0.4",
  "description": "Shared vue components for Asyncy",
  "main": "dist/asyncy-vue-components.common.js",
  "types": "src/index.d.ts",
  "web": "dist/asyncy-vue-components.js",
  "module": "es/index.js",
  "jsnext:main": "es/index.js",
  "style": "dist/css/asyncy-vue.css",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tiny-tinker/asyncy-vue.git"
  },
  "files": [
    "src",
    "dist",
    "nuxt",
    "es"
  ],
  "scripts": {
    "build": "scripts/build",
    "watch": "rollup -c scripts/rollup.config.js --watch",
    "test": "jest",
    "lint": "eslint src scripts docs tests nuxt",
    "release": "npm run build && standard-version",
    "storybook": "start-storybook -p 9002 -c .storybook",
